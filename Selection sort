void selectionsort(int arr[],int n)
{
    int i,j,small;
    for (i=0;i<n-1;i++)
    {
        small=i;
    }
    for(j=1;j<n<j++)
    {
        if(arr[j]<arr[small])
        { small=j;
            int temp=arr[j];
            arr[j]=arr[small];
            temp=arr[small]
            
        }
    }
}
